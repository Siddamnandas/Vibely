{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3002",
    "dev:tunnel": "node scripts/dev-with-tunnel.js",
    "tunnel": "node scripts/robust-tunnel.js",
    "tunnel:stop": "pkill -f robust-tunnel.js || true",
    "show-spotify-config": "node scripts/show-spotify-config.js",
    "verify-spotify-setup": "node scripts/verify-spotify-setup.js",
    "setup-spotify-redirects": "node scripts/setup-spotify-redirects.js",
    "setup-ngrok": "node scripts/setup-ngrok.js",
    "preview:mobile": "ALLOW_IFRAME_PROD=true NODE_ENV=production next build && ALLOW_IFRAME_PROD=true next start -p 3000",
    "genkit:dev": "genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "prepare": "husky",
    "format": "prettier -w .",
    "format:check": "prettier -c ."
